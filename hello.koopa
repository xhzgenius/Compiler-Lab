decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @f(%arr_param: *i32): i32 {
%0:
  @arr = alloc *i32
  store %arr_param, @arr
  %1 = getptr @arr, 1
  %2 = load %1
  ret %2
}

fun @main(): i32 {
%3:
  @arr = alloc [i32, 2]
  store {1, 2}, @arr
  %4 = getelemptr @arr, 0
  %5 = call @f(%4)
  ret %5
}
