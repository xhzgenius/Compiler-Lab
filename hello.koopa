global @x = alloc i32, 0

decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @add(%y_param: i32) {
%0:
  @y = alloc i32
  store %y_param, @y
  %1 = load @x
  %2 = load @y
  %3 = add %1, %2
  store %3, @x
  %4 = load @y
  %5 = sub %4, 1
  call @add(%5)
  ret
}

fun @main(): i32 {
%6:
  call @add(10)
  %7 = load @x
  ret %7
}
