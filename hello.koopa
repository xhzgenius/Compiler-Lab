global @x = alloc i32, 0

decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @add(%y_param: i32): i32 {
%0:
  @y = alloc i32
  store %y_param, @y
  %1 = load @y
  %2 = sub %1, 1
  %3 = call @add(%2)
  %4 = load @x
  %5 = add %3, %4
  store %5, @x
  ret 1
}

fun @main(): i32 {
%6:
  %7 = call @add(10)
  %8 = load @x
  ret %8
}
