global @a = alloc i32, zeroinit
global @b = alloc i32, zeroinit

decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @main(): i32 {
%0:
  store 10, @a
  store 5, @b
  @c = alloc i32
  %1 = load @a
  %2 = mul %1, 2
  %3 = load @b
  %4 = add %2, %3
  %5 = add %4, 3
  store %5, @c
  %6 = load @c
  ret %6
}
