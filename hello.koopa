fun @main(): i32 {
%0:
  @a_1 = alloc i32
  store 2, @a_1
  %1 = load @a_1
  br %1, %bb1_if_block_1, %bb2_if_block_2

%bb1_if_block_1:
  %2 = load @a_1
  %3 = add %2, 1
  store %3, @a_1
  jump %bb0_if_block_end

%bb2_if_block_2:
  store 0, @a_1
  jump %bb0_if_block_end

%bb0_if_block_end:
  %4 = load @a_1
  ret %4
}
