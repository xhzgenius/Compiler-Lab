fun @main(): i32 {
%entry:
  @a_1 = alloc i32
  store 2, @a_1
  %0 = load @a_1
  br %0, %if_block_1, %if_block_end

%if_block_1:
  %1 = load @a_1
  %2 = add %1, 1
  br %2, %if_block_1_0, %if_block_end_0

%if_block_1_0:
  ret

%if_block_end_0:
  jump %if_block_end

%if_block_end:
  %3 = load @a_1
  ret %3
}
